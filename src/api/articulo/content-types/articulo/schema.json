{
  "kind": "collectionType",
  "collectionName": "articulos",
  "info": {
    "singularName": "articulo",
    "pluralName": "articulos",
    "displayName": "Art√≠culos"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string",
      "required": true
    },
    "estado": {
      "type": "enumeration",
      "enum": [
        "bueno"
      ],
      "required": true
    },
    "descripcion": {
      "type": "string"
    },
    "categoria": {
      "type": "string"
    },
    "marca": {
      "type": "string"
    },
    "precio_lista": {
      "type": "decimal",
      "required": true
    },
    "unidad_de_medida": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::unidadmedida.unidadmedida"
    },
    "codigo_barras": {
      "type": "string"
    },
    "codigo_qr": {
      "type": "string"
    },
    "inventario_fiscal": {
      "type": "integer",
      "default": 0
    },
    "inventario_fisico": {
      "type": "integer",
      "default": 0
    },
    "foto": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "fresh": {
      "type": "boolean",
      "default": true
    },
    "contador": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "proveedor": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::proveedor.proveedor"
    },
    "pedidos": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::pedido.pedido",
      "mappedBy": "articulos"
    },
    "articulos_sustituto": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::articulossustituto.articulossustituto",
      "inversedBy": "articulo"
    }
  }
}
